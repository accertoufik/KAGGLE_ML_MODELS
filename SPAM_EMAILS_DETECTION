    "# Plot Precision-Recall trade-off for CountVectorizer vs TF-IDF\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.metrics import precision_recall_curve\n",
    "\n",
    "# Precision-Recall curves\n",
    "precision_count, recall_count, _ = precision_recall_curve(y_test, clf.predict_proba(X_test)[:,1])\n",
