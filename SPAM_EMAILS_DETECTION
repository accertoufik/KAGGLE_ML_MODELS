   },
   "outputs": [],
   "source": [
    "import re\n",
    "import string\n",
    "\n",
    "def preprocess_email(text):\n",
    "    text = text.lower()\n",
    "    tetx = re.sub(r\"http\\S+|www\\S+\", \"URL\", text)\n",
    "    text = re.sub(r\"\\d+\", \"NUMBER\", text)\n",
    "    text = text.translate(str.maketrans(\"\",\"\",string.punctuation))\n",
    "    return text\n",
    "\n",
    "data[\"clean_text\"] = data[\"text\"].apply(preprocess_email)"
   ]
  },
